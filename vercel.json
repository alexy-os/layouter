{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/pure-html-points/:path*",
      "destination": "/pure-html-points/:path*"
    },
    {
      "source": "/tailwind-points/:path*",
      "destination": "/tailwind-points/:path*"
    },
    {
      "source": "/tailwind-grid/:path*",
      "destination": "/tailwind-grid/:path*"
    }
  ]
} 